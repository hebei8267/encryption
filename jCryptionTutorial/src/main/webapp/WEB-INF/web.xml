<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:javaee="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<javaee:display-name>Archetype Created Web Application
	</javaee:display-name>
	
	<servlet>
		<javaee:description>EncryptionServlet</javaee:description>
		<javaee:display-name>EncryptionServlet</javaee:display-name>
		<servlet-name>EncryptionServlet</servlet-name>
		<servlet-class>encryption.EncryptionServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>EncryptionServlet</servlet-name>
		<url-pattern>/EncryptionServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<javaee:description>LoginServlet</javaee:description>
		<javaee:display-name>LoginServlet</javaee:display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>encryption.LoginServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
</web-app>